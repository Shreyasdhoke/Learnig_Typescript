//Interface

Interface usertyped{
name:string,
age:number,
address: any,
isMale:boolean}

let users:usertyped = {
name="shreyas"  
age=21
address = "India"
isMale:true
}
console.warn(users)

//------------------------

let users:any = {
name="shreyas"  
age=21
address = "India"
isMale:true
}
console.warn(users)


//--function--
function cals(a:number,b:any):number
{
return a+b
}
console.warn(cals(100,50))

//
//--function--
function cals(a:number,b?:any):number  
{
return b?a+b:a+b;
}
console.warn(cals(100)) //if second number is missing then we can use questio mark in parameter

